<p>Welcome to <code><%= @site.title %></code>!</p>

<h2>Posts</h2>

<ul id="posts-list">
  <% for post in @getCollection("posts").toJSON(): %>
    <li>
      <a href="<%= post.url %>"><time><%= post.date.toISOString() %></time>  <%= post.title %></a>
    </li>
  <% end %>
</ul>